<%- include ('layouts/header') %>
<h1 class="table">Users Details</h1>

<table class="table">
  <thead>
    <th>id</th>
    <th>email</th>
    <th>role</th>
  </thead>
  <tbody>
    <% data_Clent_data.forEach(user => { %>
    <tr>

      <td><%= user.id %></td>
      <td><a href="/superadmin/user/<%= user.id %>"><%= user.email %></a></td>
      <td><%= user.role %></td> 
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include ('layouts/footer') %>
